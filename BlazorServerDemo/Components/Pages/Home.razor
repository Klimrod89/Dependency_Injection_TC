@page "/"
@inject IDemo demo
@inject IDemo demo1
@inject ProcessDemo processDemo

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>
<h2>Demo Time: @startuptime.ToString("hh:mm:ss ffffff")</h2>
<h2>Demo Time: @startuptime1.ToString("hh:mm:ss ffffff")</h2>
<h2>Demo Time: @demo1.StartupTime.ToString("hh:mm:ss ffffff")</h2>
<h2>Demo Time: @demo.StartupTime.ToString("hh:mm:ss ffffff")</h2>
<h2>Days in Month: @processDemo.GetDaysInMonth()</h2>

@code{
    private DateTime startuptime;
    private DateTime startuptime1;
    protected override void OnInitialized()
    {
        startuptime = demo.StartupTime;
        startuptime1 = demo1.StartupTime;
    }
}
